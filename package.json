{
  "name": "neuro-game-sdk",
  "version": "1.0.10",
  "description": "A TypeScript/JavaScript SDK for integrating games with Neuro-sama AI streamer.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "browser": "dist/browser/index.umd.js",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AriesAlex/typescript-neuro-game-sdk.git"
  },
  "keywords": [
    "neuro-sama",
    "sdk",
    "typescript",
    "game",
    "integration"
  ],
  "author": "ArieX",
  "maintainers": [
    "ktrain5369"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/json-schema": "7.0.15",
    "@types/ws": "^8.18.1",
    "modern-isomorphic-ws": "^1.0.5"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "tsdown": "^0.20.3",
    "typescript": "^5.7.2"
  }
}